	Total rice yield * ((Slope rice * (Salinity levels - Threshold salinity rice))/100)
	~	ton
	~		|

chance farmer will stop farming= WITH LOOKUP (
	Savings from rice farmers,
		  ([(0,0)-(1e+07,1)],(0,1),(2000,0.5),(20000,0.1),(40000,0.05),(60000,0.03) ))
	~	dml
	~		|

chance migrating=
	0.3
	~	dml
	~		|

Chance rice farmer starting wage work=
	0.1
	~	dml
	~		|

Chance switching to vegetables=
	0.6
	~	dml
	~		|

Costs for rice seeds=
	"Rice cost price / ha"*"Land size / farmer"
	~	euros/household
	~		|

Farmers migrating=
	chance migrating*chance farmer will stop farming*Number of rice farmers
	~	household
	~		|

Farmers starting wage work=
	Chance rice farmer starting wage work*chance farmer will stop farming*Number of rice farmers
	~	household
	~		|

Income based on yield=
	"Rice yield / household"*World market rice price
	~	euros
	~		|

"Land size / farmer"=
	1.9
	~	ha
	~		|

Number of rice farmers= INTEG (
	-Farmers migrating-Farmers starting wage work-Rice farmers switching to vegetables,
		1.5e+06)
	~	household
	~		|

"Rice cost price / ha"=
	50
	~	euros/ha
	~		|

Rice farmers switching to vegetables=
	chance farmer will stop farming*Chance switching to vegetables*Number of rice farmers
	~	household
	~		|

"Rice yield / ha"=
	0.7
	~	ton/ha
	~		|

"Rice yield / household"=
	Total rice yield/Number of rice farmers
	~	ton/household
	~		|

Salinity levels=
	3
	~	dS/m
	~		|

Savings from rice farmers= INTEG (
	Income based on yield-Costs for rice seeds,
		20000)
	~	euros
	~		|

Slope rice=
	12
	~	percentage/dS/m
	~		|

Threshold salinity rice=
	3
	~	dS/m
	~		|

Total rice yield= INTEG (
